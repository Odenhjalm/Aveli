# Flutter client environment template (used by flutter_dotenv).
# Copy to frontend/.env for local runs; set via --dart-define in CI/builds.

API_BASE_URL=http://127.0.0.1:8080
# Android emulator cannot reach 127.0.0.1 on host; use http://10.0.2.2:8080 there.

# Supabase client auth
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=replace-with-anon-key

# Stripe PaymentSheet/Checkout (publishable only)
STRIPE_PUBLISHABLE_KEY=pk_test_replace
STRIPE_MERCHANT_DISPLAY_NAME=Aveli

# Feature flags
SUBSCRIPTIONS_ENABLED=true
IMAGE_LOGGING=true
